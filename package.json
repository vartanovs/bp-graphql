{
  "name": "graphql-server-boilerplate",
  "version": "1.0.0",
  "description": "GraphQL Server Boilerplate",
  "main": "src/index.ts",
  "scripts": {
    "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts",
    "start": "NODE_ENV=dev nodemon --exec ts-node src/index.ts",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vartanovs/graphql-server-boilerplate.git"
  },
  "keywords": [
    "GraphQL",
    "PostgreSQL",
    "Node",
    "Jest"
  ],
  "author": "Serge Vartanov <vartanov.s@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vartanovs/graphql-server-boilerplate/issues"
  },
  "homepage": "https://github.com/vartanovs/graphql-server-boilerplate#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^4.0.2",
    "graphql-yoga": "^1.16.7",
    "ioredis": "^4.2.0",
    "node-fetch": "^2.2.0",
    "pg": "^7.5.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.8",
    "uuid": "^3.3.2",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/ioredis": "^4.0.3",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.2",
    "@types/uuid": "^3.4.4",
    "@types/yup": "^0.26.0",
    "gql2ts": "^1.10.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3"
  }
}
